{
  "manifest_version": 3,
  "name": "Karsaz",
  "version": "1.0",
  "description": "افزونه ورود و ساخت فعالیت در دیدار",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
  "service_worker": "background.js"
},
  "permissions": ["storage"],
  "host_permissions": ["https://app.didar.me/*"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
